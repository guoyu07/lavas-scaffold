{
  "name": "lavas-scaffold",
  "version": "1.0.5",
  "description": "PWA project scaffold solution",
  "main": "index.js",
  "scripts": {
    "pretest": "npm run fixtures:rebuild && npm run lint",
    "test": "nyc ava",
    "test:watch": "jest --watch",
    "lint": "fecs --ignore=./.gitignore bin lib test --rule",
    "build": "sh build.sh",
    "fixtures:ensure": "cross-env NODE_ENV=development babel-node ./test/helpers/setupFixtures.js",
    "fixtures:rebuild": "cross-env NODE_ENV=development babel-node ./test/helpers/cleanupFixtures.js && npm run fixtures:ensure"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lavas-project/lavas-scaffold.git"
  },
  "keywords": [
    "pwa",
    "vue",
    "lavas",
    "scaffold"
  ],
  "author": "sfe-sy<sfe-sy@baidu.com>",
  "license": "ISC",
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "ava": {
    "concurrency": 5,
    "failFast": true,
    "files": [
      "test/**/*.test.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "dependencies": {
    "ajv": "^5.1.3",
    "archiver": "^1.3.0",
    "babel-runtime": "^6.23.0",
    "chalk": "^1.1.3",
    "etpl": "^3.2.0",
    "fs-extra": "^3.0.1",
    "glob": "^7.1.2",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "shelljs": "^0.7.8",
    "simple-git": "^1.73.0"
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-cli": "^6.24.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.3.2",
    "babel-register": "^6.23.0",
    "cross-env": "^4.0.0",
    "fecs": "^1.3.1",
    "jest": "^19.0.2",
    "nyc": "^10.3.2",
    "sinon": "^2.4.1"
  }
}
